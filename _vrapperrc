imap jk <esc>
map <c-h> gT
map <c-l> gt
map H ^
map L $
set autoindent
set ignorecase
set smartcase
set gvimpath="C:\Program Files (x86)\Vim\vim80\gvim.exe"
let mapleader=','
map <leader>f :f<cr>

